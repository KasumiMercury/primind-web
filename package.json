{
    "name": "primind-web",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "react-router build",
        "build:cf": "VITE_RUNTIME=cloudflare react-router build",
        "dev": "react-router dev",
        "dev:cf": "VITE_RUNTIME=cloudflare react-router build && wrangler dev",
        "start": "react-router-serve ./build/server/index.js",
